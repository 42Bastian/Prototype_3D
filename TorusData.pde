void setupTorus()
{
  torus = new Object(lx, ly, lz, torus_faces.length/3);
  torus2 = new Object(lx, ly, lz, torus_faces.length/3);

  for (int i = 0, in = 0, t = 0; i < torus_faces.length; ++t ) {
    int pi2 = torus_faces[i++];
    int pi1 = torus_faces[i++];
    int pi3 = torus_faces[i++];
    dot3d n;

    //dot3d n0 = new dot3d(torus_normals[in++], torus_normals[in++], torus_normals[in++]);

    dot3d p1 = new dot3d(lx[pi1], ly[pi1], lz[pi1]);
    dot3d p2 = new dot3d(lx[pi2], ly[pi2], lz[pi2]);
    dot3d p3 = new dot3d(lx[pi3], ly[pi3], lz[pi3]);
    n = calculateNormal(p1, p2, p3);
    //println(" NT ", n.x, ",", n.y, ",", n.z);
    int col = 200<<16;//(120+(i&1)*20)<<16;//int(200/36.*n)+55;
    torus.normals[t] = n;
    if ( t < 24 ) col = (255<<16)+255;

    if ( t >= 272 & t <= 275 ) col = 128<<8;
    torus.addTriangle(new Tri(pi1, pi2, pi3, col));

    col = (255)|(255<<8);
    torus2.normals[t] = n;
    if ( t == 0 ) col = 255<<16|120;

    torus2.addTriangle(new Tri(pi1, pi2, pi3, col));
  }
  for (int vi = 0, i = 0; i < lx.length; ++i) {
    dot3d vn = new dot3d(torus_vnormals[vi++],
      torus_vnormals[vi++],
      torus_vnormals[vi++]);
    vn.normalize();
    //println(" VNT ",vn.x,",",vn.y,",",vn.z);
    torus.vnormals[i] = vn;
    torus2.vnormals[i] = vn;
  }

  torus.setPos(new dot3d(0, 100, 0));
  torus2.setPos(new dot3d(0, 00, 800));
}
